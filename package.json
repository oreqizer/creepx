{
  "name": "creepx",
  "version": "0.2.0",
  "description": "Declarative user event tracking system. :squirrel:",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "npm run test:base | faucet",
    "test:nyan": "npm run test:base | tap-nyan",
    "test:base": "tape -r babel-register 'src/**/__tests__/*.spec.js'",
    "test:coverage": "nyc --reporter=lcov > coverage.lcov npm run test:base",
    "test:flow": "flow check",
    "lint": "eslint './**/*.js'",
    "build": "gulp build",
    "clean": "rimraf es lib",
    "prepare": "npm run clean && npm test && npm run test:flow && npm run lint && npm run build"
  },
  "keywords": [
    "tracking"
  ],
  "author": "oreqizer",
  "license": "MIT",
  "dependencies": {
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "faucet": "^0.0.1",
    "flow-bin": "^0.59.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-plumber": "^1.1.0",
    "nyc": "^11.3.0",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.2",
    "tap-nyan": "^1.1.0",
    "tape": "^4.8.0"
  },
  "files": [
    "es",
    "lib",
    "README.md"
  ]
}
