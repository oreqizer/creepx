{
  "name": "creepx",
  "version": "0.4.0",
  "description": "Declarative user event tracking system. :squirrel:",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "tape -r babel-register 'src/**/__tests__/*.spec.js'",
    "test:nyan": "npm test | tap-nyan",
    "test:faucet": "npm test | faucet",
    "test:coverage": "nyc --reporter=lcov > coverage.lcov npm test",
    "lint": "eslint './**/*.js'",
    "clean": "rimraf es lib",
    "prepare": "yarn clean && npm test && yarn flow check && yarn lint && yarn gulp build"
  },
  "keywords": [
    "tracking"
  ],
  "author": "oreqizer",
  "license": "MIT",
  "dependencies": {
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "faucet": "^0.0.1",
    "flow-bin": "^0.76.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.1",
    "gulp-plumber": "^1.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tap-nyan": "^1.1.0",
    "tape": "^4.9.1"
